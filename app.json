{
  "expo": {
    "name": "sunny",
    "slug": "sunny",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashNoAni.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF9EF"
    },
    "assetBundlePatterns": ["**/*"],
    "notification": {
      "icon": "./assets/app_icon.png"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.sunny-money.SUNNY"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF9EF"
      },
      "versionCode": 25,
      "permissions": ["CAMERA", "NOTIFICATIONS"],
      "package": "com.sunny.inout_webview",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "99b2fa8d-2739-4de2-b65f-991835ff78b3"
      }
    },
    "plugins": ["expo-apple-authentication"],
    "owner": "suyoooi"
  }
}
